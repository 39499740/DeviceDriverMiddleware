<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  
  <appSettings>
    <!-- WebSocket服务器配置 -->
    <add key="WebSocketPort" value="45677" />
    <add key="WebSocketHost" value="localhost" />
    
    <!-- HTTP服务器配置 -->
    <add key="HttpPort" value="45677" />
    <add key="HttpHost" value="localhost" />
    
    <!-- 日志配置 -->
    <add key="LogLevel" value="Info" />
    <add key="LogToFile" value="true" />
    <add key="LogFilePath" value="logs\twain-middleware.log" />
    
    <!-- TWAIN配置 -->
    <add key="DefaultResolution" value="300" />
    <add key="DefaultColorMode" value="Color" />
    <add key="DefaultFormat" value="PNG" />
    
    <!-- 系统配置 -->
    <add key="AutoStartService" value="true" />
    <add key="MinimizeToTray" value="true" />
    <add key="ShowTrayNotifications" value="true" />
  </appSettings>
  
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-13.0.0.0" newVersion="13.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration> 